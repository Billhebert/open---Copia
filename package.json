{
  "name": "opencode-dashboard",
  "version": "1.0.0",
  "description": "Dashboard Web com API REST para Opencode AI",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "tsx watch src/main.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "build:legacy": "tsc && node dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "install:all": "npm install && cd frontend && npm install",
    "setup": "tsx scripts/setup.ts",
    "seed:demo": "tsx scripts/seed-demo.ts"
  },
  "keywords": [
    "opencode",
    "ai",
    "dashboard",
    "api",
    "rag",
    "auth",
    "mcp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@opencode-ai/sdk": "^1.1.8",
    "@prisma/client": "^6.1.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.0.4",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.2",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
